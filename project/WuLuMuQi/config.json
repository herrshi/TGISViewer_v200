{
  "theme": {
    "name": "BillboardTheme",
    "styles": [
      "default",
      "blue",
      "purple",
      "turquoise",
      "green",
      "orange",
      "red",
      "pink"
    ],
    "version": "2.4"
  },
  "map": {
    "3D": false,
    "2D": true,
    "position": {
      "left": 0,
      "top": 0,
      "right": 0,
      "bottom": 0
    },
    "basemaps": [
      {
        "label": "矢量图",
        "url": "https://map.geoq.cn/arcgis/rest/services/ChinaOnlineStreetPurplishBlue/MapServer",
        "type": "tiled",
        "visible": true
      }
    ],
    "operationallayers": [
      {
        "label": "SXJ",
        "url": "http://{gisServer}/arcgis/rest/services/WuLuMuQi/wlmq_device/MapServer/0",
        "type": "feature",
        "visible": true,
        "outFields": ["*"],
        "infoTemplate": {
          "title": "${DEVICEID}",
          "content": "<b>设备描述: </b>${DEVICEDESC}<br><b>设备类型: </b>${DEVICETYPE}<br><b>"
        },
        "renderer": {
          "type": "simple",
          "symbol": {
            "type": "esriPMS",
            "url": "images/mapIcons/SheXiangJi-blue.png",
            "width": 22,
            "height": 26,
            "yoffset": 16
          }
        }
      },
      {
        "label": "kk",
        "url": "http://{gisServer}/arcgis/rest/services/WuLuMuQi/wlmq_device/MapServer/1",
        "type": "feature",
        "visible": true,
        "outFields": ["*"],
        "infoTemplate": {
          "title": "${DEVICEID}",
          "content": "<b>设备描述: </b>${DEVICEDESC}<br><b>设备类型: </b>${DEVICETYPE}<br><b>"
        },
        "renderer": {
          "type": "simple",
          "symbol": {
            "type": "esriPMS",
            "url": "images/mapIcons/KaKou-blue.png",
            "width": 22,
            "height": 26,
            "yoffset": 16
          }
        }
      },
      {
        "label": "RFID",
        "url": "http://{gisServer}/arcgis/rest/services/WuLuMuQi/wlmq_device/MapServer/2",
        "type": "feature",
        "visible": true,
        "outFields": ["*"],
        "infoTemplate": {
          "title": "${DEVICEID}",
          "content": "<b>设备描述: </b>${DEVICEDESC}<br><b>设备类型: </b>${DEVICETYPE}<br><b>"
        },
        "renderer": {
          "type": "simple",
          "symbol": {
            "type": "esriPMS",
            "url": "images/mapIcons/icon_camera_blue.png",
            "width": 22,
            "height": 26,
            "yoffset": 16
          }
        }
      },
      {
        "label": "SP",
        "url": "http://{gisServer}/arcgis/rest/services/WuLuMuQi/wlmq_device/MapServer/3",
        "type": "feature",
        "visible": true,
        "outFields": ["*"],
        "infoTemplate": {
          "title": "${DEVICEID}",
          "content": "<b>设备描述: </b>${DEVICEDESC}<br><b>设备类型: </b>${DEVICETYPE}<br><b>"
        },
        "renderer": {
          "type": "simple",
          "symbol": {
            "type": "esriPMS",
            "url": "images/mapIcons/XinHaoJi-blue.png",
            "width": 22,
            "height": 26,
            "yoffset": 16
          }
        }
      }

    ],
    "mapOptions": {
      "center": [87.597, 43.824],
      "scale": 144448,
      "logo": false,
      "nav": false,
      "slider": false
    }
  },
  "geometryService": "http://{gisServer}/arcgis/rest/services/Utilities/Geometry/GeometryServer",
  "gisServer": "172.10.10.1:6080",
  "widgetOnScreen": {
    "widgets": [
      {
        "uri": "widgets/Coordinate/Widget",
        "position": {
          "left": 15,
          "bottom": 5,
          "relativeTo": "map"
        },
        "version": "2.4",
        "id": "CoordinateWidget"
      },
      {
        "uri": "widgets/OverviewMap/Widget",
        "position": {
          "right": 0,
          "bottom": 0,
          "relativeTo": "map"
        },
        "version": "2.4",
        "id": "OverviewMapWidget"
      },
      {
        "uri": "widgets/Overlay/Widget",
        "version": "2.4",
        "id": "OverlayWidget"
      },
      {
        "uri": "widgets/GeometrySearch/Widget",
        "version": "2.4",
        "name": "GeometrySearch",
        "config": "configs/GeometrySearch/config.json",
        "position": {
          "left": 15,
          "top": 190,
          "relativeTo": "map"
        },
        "id": "GeometrySearchWidget"
      },
      {
        "uri": "widgets/TrackPlayback/Widget",
        "config": "configs/TrackPlayback/config_ShangHaiHarbour.json",
        "id": "TrackPlaybackWidget"
      },
      {
        "uri": "widgets/DrawOverlay/Widget",
        "version": "2.4",
        "id": "DrawOverlayWidget"
      },
      {
        "uri": "widgets/HeatMap/Widget",
        "version": "2.4",
        "id": "HeatMapWidget"
      },
      {
        "uri": "widgets/FindFeature/Widget",
        "version": "2.4",
        "id": "FindFeatureWidget"
      },
      {
        "uri": "widgets/Cluster/Widget",
        "version": "2.4",
        "id": "ClusterWidget"
      },
      {
        "uri": "widgets/Monitor/Widget",
        "version": "2.4",
        "id": "MonitorWidget"
      },
      {
        "uri": "widgets/ToolTip/Widget",
        "config": "configs/ToolTip/config_WuLuMuQi.json",
        "version": "2.4",
        "id": "ToolTipWidget"
      }
    ]
  }
}
